<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="Project">
	<TargetFramework>net5.0</TargetFramework>
	<LangVersion>8.0</LangVersion>
    <AssemblyTitle>Vitaru.Server</AssemblyTitle>
    <Description>Vitaru's Server</Description>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <OutputType>Exe</OutputType>
    <StartupObject />
    <Configurations>Debug;Release;Publish</Configurations>
    <Platform>AnyCPU</Platform>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Publish|AnyCPU'">
    <DefineConstant>PUBLISH</DefineConstant>
    <Optimize>true</Optimize>
	<SignAssembly>true</SignAssembly>
	<DelaySign>false</DelaySign>
	<AssemblyOriginatorKeyFile>..\..\Sym.pfx</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>vitaru.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Prion\Prion.Core\Prion.Core.csproj" />
  </ItemGroup>
</Project>