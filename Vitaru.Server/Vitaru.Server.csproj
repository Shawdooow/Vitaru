    <LangVersion>8.0</LangVersion>
    <AssemblyTitle>Vitaru.Server</AssemblyTitle>
    <Description>Vitaru's Server</Description>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <OutputType>Exe</OutputType>
    <StartupObject />
